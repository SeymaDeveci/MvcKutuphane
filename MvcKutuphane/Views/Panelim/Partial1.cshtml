@using MvcKutuphane.Models.Entity
@model List<TBLDUYURULAR>

@foreach (var x in Model)
{
    <div class="post">
        <div class="user-block">
            <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/2.png" alt="user image">
            <span class="kategori">
                <a href="#">@x.KATEGORI</a>
            </span>

            <span class="description">@Convert.ToDateTime(x.TARIH).ToString("dd/MM/yyyy")</span>

        </div>
        <p>@x.ICERİK</p>
    </div>
}