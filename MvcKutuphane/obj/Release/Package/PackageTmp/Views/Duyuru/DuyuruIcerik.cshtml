@using MvcKutuphane.Models.Entity
@model List<TBLDUYURULAR>
@{
    ViewBag.Title = "DuyuruIcerik";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h3>@ViewBag.dyrad</h3>
<br />
<table class="table table-striped">
    <tr>
        <th>Duyuru No</th>
        <th>Duyuru İçeriği</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.ICERİK</td>
        </tr>
    }
</table>


